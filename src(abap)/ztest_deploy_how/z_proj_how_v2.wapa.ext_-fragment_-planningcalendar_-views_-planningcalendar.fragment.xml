<core:FragmentDefinition xmlns:f="sap.f" xmlns:unified="sap.ui.unified" xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:macros="sap.fe.macros" >                                                                                                                  
    <!-- Remove IDs for Single Planning Calendar to work -->                                                                                                                                                                                                   
    <Page&nbsp;                                                                                                                                                                                                                                                
        id="_IDGenPage1"&nbsp;                                                                                                                                                                                                                                 
        showFooter="true"&nbsp;                                                                                                                                                                                                                                
        showHeader="false"                                                                                                                                                                                                                                     
        core:require="{ handler: 'projectsdemoappv2/projectsdemoappv2/ext/fragment/PlanningCalendar/handler/PlanningCalendarHandler',                                                                                                                          
                        formatter: 'projectsdemoappv2/projectsdemoappv2/ext/formatter/PlanningCalendarFormatter' }">                                                                                                                                           
    <!-- <Panel id="_IDGenPanel1"&nbsp;                                                                                                                                                                                                                        
        headerText="Projects Calendar View"                                                                                                                                                                                                                    
        class="sapUiResponsiveMargin"                                                                                                                                                                                                                          
        width="auto"                                                                                                                                                                                                                                           
        core:require="{ handler: 'projectsdemoappv2/projectsdemoappv2/ext/fragment/PlanningCalendar/handler/PlanningCalendarHandler',                                                                                                                          
                        formatter: 'projectsdemoappv2/projectsdemoappv2/ext/formatter/PlanningCalendarFormatter' }"                                                                                                                                            
        > -->                                                                                                                                                                                                                                                  
        <SinglePlanningCalendar                                                                                                                                                                                                                                
            class="sapUiSmallMarginTop"                                                                                                                                                                                                                        
            title="SMIC Projects"                                                                                                                                                                                                                              
            startHour="9"                                                                                                                                                                                                                                      
            endHour="19"                                                                                                                                                                                                                                       
            fullDay="false"                                                                                                                                                                                                                                    
            appointmentSelect="handler.selectedAppointment"                                                                                                                                                                                                    
            moreLinkPress="handler.moreLinkHandler"                                                                                                                                                                                                            
            appointments="{/Projects}"                                                                                                                                                                                                                         
            >                                                                                                                                                                                                                                                  
            <views>                                                                                                                                                                                                                                            
				<SinglePlanningCalendarMonthView id="_IDGenSinglePlanningCalendarMonthView1" key="MonthView" title="Month" />                                                                                                                                              
				<SinglePlanningCalendarWorkWeekView id="_IDGenSinglePlanningCalendarWorkWeekView1" key="WorkWeekView" title="Work Week" />                                                                                                                                 
				<SinglePlanningCalendarWeekView id="_IDGenSinglePlanningCalendarWeekView1" key="WeekView" title="Week" />                                                                                                                                                  
                <SinglePlanningCalendarDayView id="_IDGenSinglePlanningCalendarDayView1" key="DayView" title="Day" />                                                                                                                                          
            </views>                                                                                                                                                                                                                                           
            <actions>                                                                                                                                                                                                                                          
                <Button id="_IDGenButton1" icon="sap-icon://zoom-in" press="handler.zoomIn"/>                                                                                                                                                                  
				<Button id="_IDGenButton2" icon="sap-icon://zoom-out" press="handler.zoomOut"/>                                                                                                                                                                            
            </actions>                                                                                                                                                                                                                                         
            <appointments>                                                                                                                                                                                                                                     
                <unified:CalendarAppointment&nbsp;                                                                                                                                                                                                             
                    title="{formatter: 'formatter.formatProjectName', path: 'ProjectName'}"                                                                                                                                                                    
                    startDate="{formatter: 'formatter.formatCalendarDateTime', path: 'StartDate'}"                                                                                                                                                             
                    endDate="{path: 'ProjectedDate'}">                                                                                                                                                                                                         
                </unified:CalendarAppointment>                                                                                                                                                                                                                 
            </appointments>                                                                                                                                                                                                                                    
        </SinglePlanningCalendar>                                                                                                                                                                                                                              
    <!-- </Panel> -->                                                                                                                                                                                                                                          
    </Page>                                                                                                                                                                                                                                                    
</core:FragmentDefinition>                                                                                                                                                                                                                                     