sap.ui.require(                                                                                                                                                                                                                                                
    [                                                                                                                                                                                                                                                          
        'sap/fe/test/JourneyRunner',                                                                                                                                                                                                                           
        'odatav4test2/test/integration/FirstJourney',                                                                                                                                                                                                          
		'odatav4test2/test/integration/pages/ProjectsList',                                                                                                                                                                                                          
		'odatav4test2/test/integration/pages/ProjectsObjectPage',                                                                                                                                                                                                    
		'odatav4test2/test/integration/pages/ObjectsObjectPage'                                                                                                                                                                                                      
    ],                                                                                                                                                                                                                                                         
    function(JourneyRunner, opaJourney, ProjectsList, ProjectsObjectPage, ObjectsObjectPage) {                                                                                                                                                                 
        'use strict';                                                                                                                                                                                                                                          
        var JourneyRunner = new JourneyRunner({                                                                                                                                                                                                                
            // start index.html in web folder                                                                                                                                                                                                                  
            launchUrl: sap.ui.require.toUrl('odatav4test2') + '/index.html'                                                                                                                                                                                    
        });                                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                                     
        JourneyRunner.run(                                                                                                                                                                                                                                     
            {                                                                                                                                                                                                                                                  
                pages: {&nbsp;                                                                                                                                                                                                                                 
					onTheProjectsList: ProjectsList,                                                                                                                                                                                                                          
					onTheProjectsObjectPage: ProjectsObjectPage,                                                                                                                                                                                                              
					onTheObjectsObjectPage: ObjectsObjectPage                                                                                                                                                                                                                 
                }                                                                                                                                                                                                                                              
            },                                                                                                                                                                                                                                                 
            opaJourney.run                                                                                                                                                                                                                                     
        );                                                                                                                                                                                                                                                     
    }                                                                                                                                                                                                                                                          
);                                                                                                                                                                                                                                                             