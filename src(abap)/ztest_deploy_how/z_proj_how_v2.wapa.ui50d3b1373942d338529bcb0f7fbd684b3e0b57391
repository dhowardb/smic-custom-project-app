"use strict";                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
sap.ui.define(["sap/m/MessageToast"], function (MessageToast) {                                                                                                                                                                                                
  "use strict";                                                                                                                                                                                                                                                
                                                                                                                                                                                                                                                               
  function handleDialogCancel(event) {                                                                                                                                                                                                                         
    var dialog = event.getSource().getParent();                                                                                                                                                                                                                
    if (dialog.isOpen()) {                                                                                                                                                                                                                                     
      dialog.close();                                                                                                                                                                                                                                          
    }                                                                                                                                                                                                                                                          
    var view = this.editFlow.getView();                                                                                                                                                                                                                        
    var model = view.getModel();                                                                                                                                                                                                                               
    model.resetChanges();                                                                                                                                                                                                                                      
  }                                                                                                                                                                                                                                                            
  var handleDialogUpdate = function handleDialogUpdate(event) {                                                                                                                                                                                                
    MessageToast.show('Update Button was clicked! Posting...');                                                                                                                                                                                                
    var dialog = event.getSource().getParent();                                                                                                                                                                                                                
    var bindingContext = dialog.getBindingContext();                                                                                                                                                                                                           
    bindingContext.refresh();                                                                                                                                                                                                                                  
    if (bindingContext.hasPendingChanges()) {                                                                                                                                                                                                                  
      MessageToast.show('has pending Changes');                                                                                                                                                                                                                
    }                                                                                                                                                                                                                                                          
    var form = dialog.getAggregation('content');                                                                                                                                                                                                               
    // form.                                                                                                                                                                                                                                                   
  };                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
  var handleDialogDelete = function handleDialogDelete(event) {                                                                                                                                                                                                
    MessageToast.show('Delete Button was clicked! Deleting...');                                                                                                                                                                                               
  };                                                                                                                                                                                                                                                           
  function handleDatePickerChange(event, pageContext) {                                                                                                                                                                                                        
    var source = event.getSource();                                                                                                                                                                                                                            
    var label = source.getParent();                                                                                                                                                                                                                            
    var formElements = label.getParent().getAggregation('formElements');                                                                                                                                                                                       
    var startDate = formElements[1].getAggregation('fields').reduce(function (field) {                                                                                                                                                                         
      return field;                                                                                                                                                                                                                                            
    }).getValue();                                                                                                                                                                                                                                             
    var endDate = formElements[2].getAggregation('fields').reduce(function (field) {                                                                                                                                                                           
      return field;                                                                                                                                                                                                                                            
    }).getValue();                                                                                                                                                                                                                                             
    var form = label.getParent().getParent();                                                                                                                                                                                                                  
    var dialog = form.getParent().getParent().getParent();                                                                                                                                                                                                     
    var updateButton = dialog.getAggregation('beginButton');                                                                                                                                                                                                   
    if (endDate < startDate) {                                                                                                                                                                                                                                 
      if (updateButton.getEnabled()) {                                                                                                                                                                                                                         
        updateButton.setEnabled(false);                                                                                                                                                                                                                        
      }                                                                                                                                                                                                                                                        
      MessageToast.show('Cannot update! Start Date should be before End Date ');                                                                                                                                                                               
      return;                                                                                                                                                                                                                                                  
    }                                                                                                                                                                                                                                                          
    updateButton.setEnabled(true);                                                                                                                                                                                                                             
    var view = this.editFlow.getView();                                                                                                                                                                                                                        
    var model = view.getModel();                                                                                                                                                                                                                               
    var hasPendingChanges = model.hasPendingChanges();                                                                                                                                                                                                         
    var updateGroupID = model.getUpdateGroupId();                                                                                                                                                                                                              
    if (hasPendingChanges) {                                                                                                                                                                                                                                   
      // model.resetChanges();                                                                                                                                                                                                                                 
    }                                                                                                                                                                                                                                                          
    // model.refresh();                                                                                                                                                                                                                                        
  }                                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
  var handleModelContextChange = function handleModelContextChange(event) {                                                                                                                                                                                    
    var source = event.getSource();                                                                                                                                                                                                                            
    var parameters = event.getParameters();                                                                                                                                                                                                                    
  };                                                                                                                                                                                                                                                           
  var __exports = {                                                                                                                                                                                                                                            
    __esModule: true                                                                                                                                                                                                                                           
  };                                                                                                                                                                                                                                                           
  __exports.handleDialogCancel = handleDialogCancel;                                                                                                                                                                                                           
  __exports.handleDialogUpdate = handleDialogUpdate;                                                                                                                                                                                                           
  __exports.handleDialogDelete = handleDialogDelete;                                                                                                                                                                                                           
  __exports.handleDatePickerChange = handleDatePickerChange;                                                                                                                                                                                                   
  __exports.handleModelContextChange = handleModelContextChange;                                                                                                                                                                                               
  return __exports;                                                                                                                                                                                                                                            
});                                                                                                                                                                                                                                                            